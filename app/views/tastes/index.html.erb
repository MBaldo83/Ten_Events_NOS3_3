<h1>An index of the saved Tastes</h1>

<table>
  <tr>
    <th>Tastes</th>
  </tr>

<% @all_tastes.each do |taste| %>
  <tr>
    <td><%=h taste.genre %></td>
   	<td><%= link_to "Delete", taste, :method => :delete, :confirm => 'Are you sure?',  
									 :title => "Delete Taste" %></td>
  </tr>
<% end %>
</table>